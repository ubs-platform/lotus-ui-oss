{
  "name": "tk-santral",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/admin/tk-santral/src",
  "prefix": "tk-santral",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/tk-santral",
        "index": "apps/admin/tk-santral/src/index.html",
        "main": "apps/admin/tk-santral/src/main.ts",
        "polyfills": "apps/admin/tk-santral/src/polyfills.ts",
        "tsConfig": "apps/admin/tk-santral/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/admin/tk-santral/src/favicon.ico",
          "apps/admin/tk-santral/src/assets",
          {
            "glob": "**/*",
            "input": "node_modules/monaco-editor",
            "output": "/assets/monaco/"
          },
          {
            "glob": "**/*",
            "input": "common/localization",
            "output": "localization"
          },
          {
            "glob": "**/*",
            "input": "node_modules/chart.js/dist",
            "output": "dependencies/chart-js"
          },
          {
            "glob": "**/*",
            "input": "node_modules/katex/dist/",
            "output": "dependencies/katex"
          },
          {
            "glob": "**/*",
            "input": "node_modules/quill/dist",
            "output": "dependencies/quill"
          }
        ],
        "scripts": [],
        "styles": ["apps/admin/tk-santral/src/styles.scss"]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "3.7mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "26kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "environment/environment.ts",
              "with": "environment/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "tk-santral:build:production"
        },
        "development": {
          "port": 4500,
          "buildTarget": "tk-santral:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "proxyConfig": "apps/admin/tk-santral/proxy.conf.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "tk-santral:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/admin/tk-santral"],
      "options": {
        "jestConfig": "apps/admin/tk-santral/jest.config.ts"
      }
    }
  }
}
