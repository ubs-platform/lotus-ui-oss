{
  "name": "front-global-gui-primitive",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/front-global/gui-primitive/src",
  "prefix": "lotus-web",
  "tags": [],
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/front-global/gui-primitive"],
      "options": {
        "jestConfig": "libs/front-global/gui-primitive/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "libs/front-global/gui-primitive/**/*.ts",
          "libs/front-global/gui-primitive/**/*.html"
        ]
      }
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/front-global/gui-primitive/.storybook",
        "browserTarget": "front-global-gui-primitive:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/front-global-tui-blocks",
        "configDir": "libs/front-global/gui-primitive/.storybook",
        "browserTarget": "front-global-gui-primitive:build-storybook",
        "compodoc": false,
        "styles": ["apps/lotus/src/styles.scss"]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  }
}
