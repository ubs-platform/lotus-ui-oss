  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js",
    "start:lotus": "nx run-many --projects=lotus,lotus-api-exams,'tk-santral',postral-core --target=serve --parallel --inspect",
    "start:lotus:frontend": "nx serve lotus --host 0.0.0.0 --disable-host-check",
    "start:planor:frontend": "nx serve planor --host 0.0.0.0 --disable-host-check --port=4201",
    "start:postral:frontend": "nx serve postral-core --host 0.0.0.0 --disable-host-check --port=4204",
    "start:monaui-tools": "nx serve monaui-tools --host 0.0.0.0 --disable-host-check --port=4289",
    "start:lotus:backend-all": "nx run-many --projects=api-ubs-files,lotus-api-exams --target=serve --inspect",
    "start:lotus:backend": "nx serve lotus-api-exams",
    "start:tk-santral:frontend": "nx run tk-santral:serve:development  --host 0.0.0.0 --allowed-hosts all --port=4500",
    "start:chatbox:frontend": "nx serve tk-ai-chatbox --host 0.0.0.0 --disable-host-check --port 4203",
    "build": "nx build",
    "test": "nx test",
    "test:open": "cd apps/lotus-e2e; npx cypress open",
    "start:backend-full": "nx run-many --projects=users,api-ubs-files,lotus-api-exams --target=serve"
  },
